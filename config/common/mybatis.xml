		<typeAlias alias="PrtyConfig" type="com.ai.wocloud.bean.PrtyConfig"/>
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/loginInfo/loginInfo.xml" />
		<mapper resource="mybatis/companyInfo/companyInfo.xml" />
		<mapper resource="mybatis/softwareInfo/software.xml" />
		<mapper resource="mybatis/softwareInfo/applyodd.xml" />
		<mapper resource="mybatis/softwareInfo/vminfo.xml" />
		<mapper resource="mybatis/softwareInfo/softwareType.xml" />
